        },
        {
          "type": "code",
          "language": "python",
          "content": "cols = [\"home_team\", \"away_team\", \"week\", \"game_seconds_remaining\", \"down\", \"yards_gained\", \"passer_player_name\", \"rusher_player_name\", \"receiver_player_name\", \"home_coach\", \"away_coach\", \"temp\", \"wind\"]\nplay_by_play[cols].head()"
